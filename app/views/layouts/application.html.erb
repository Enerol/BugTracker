<!DOCTYPE html>
<html>
<head>
  <title><%= title %></title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
  <div id="wrapper">
    <%= render "layouts/header" %>
    <div id="mainContent">
      <%=render "layouts/sidebar" %>

      <%=render "layouts/flash_messages"%>
      <div id="pageContent">
        <%= yield %>
      </div>
      <div class="clear">&nbsp;</div>
    </div>
    
  </div>
  <%= render "layouts/footer" %>
</body>
</html>
